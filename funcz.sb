let rotrrr = gen() {
	setup() {
	  let notes = ["c3", "g2" , "c2", "f0" ]
	  let idx = 0

 	  let counts = ["3", "12", "4", "13", "7"]
	  let count_idx = 0

	}

	run() {
	  let note = notes[idx]
	  let idx = idx + 1
	  if (idx > len(notes) - 1) { let idx = 0}

	  let count = counts[count_idx]
	  let count_idx = count_idx + 1
	  if (count_idx > len(counts) - 1) { let count_idx = 0}

          return note + "*" + count

	}
}
