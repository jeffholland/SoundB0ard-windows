let rotrrr = gen() {
	setup() {
	  let pat = ["b1", "d1", "e1", "f1", "g2"]
          let countz = ["3", "4", "7", "2"]
	  let idx = 0
	  let count_idx = 0
	}

	run() {
	  let note = pat[idx]
	  let idx = idx + 1
	  if (idx > len(pat) - 1) { let idx = 0}

          let count = countz[count_idx]
          let count_idx = count_idx + 1
	  if (count_idx > len(countz) - 1) { let count_idx = 0}
      

          return note + "*" + count

	}
}
